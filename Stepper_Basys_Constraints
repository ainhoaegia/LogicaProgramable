## Clock signal

set_property PACKAGE_PIN W5 [get_ports clk]							

	set_property IOSTANDARD LVCMOS33 [get_ports clk]

	create_clock -add -name sys_clk_pin -period 10.00 -waveform {0 5} [get_ports clk]

 

## Switches

set_property PACKAGE_PIN V17 [get_ports {enable}]					

	set_property IOSTANDARD LVCMOS33 [get_ports {enable}]

set_property PACKAGE_PIN V16 [get_ports {sentido}]					

	set_property IOSTANDARD LVCMOS33 [get_ports {sentido}]

#set_property PACKAGE_PIN W16 [get_ports {sw[2]}]					

	#set_property IOSTANDARD LVCMOS33 [get_ports {sw[2]}]

 

##Buttons

set_property PACKAGE_PIN U18 [get_ports reset]						

	set_property IOSTANDARD LVCMOS33 [get_ports reset]


##Pmod Header JA

##Sch name = JA1

set_property PACKAGE_PIN J1 [get_ports {paso_paso[0]}]				

	set_property IOSTANDARD LVCMOS33 [get_ports {paso_paso[0]}]

##Sch name = JA2

set_property PACKAGE_PIN L2 [get_ports {paso_paso[1]}]				

	set_property IOSTANDARD LVCMOS33 [get_ports {paso_paso[1]}]

